{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.manova.life" }],
      "destination": "https://manova.life/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|_next/static|_next/image|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
